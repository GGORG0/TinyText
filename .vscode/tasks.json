{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "Makefile: run task all",
      "command": "/usr/bin/make",
      "args": [
        "all"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ],
  "version": "2.0.0"
}